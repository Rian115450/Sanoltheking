.option-animate-image-fade .sb-animate-image:hover { opacity: 0.7; }

.option-animate-image-grayscale .sb-animate-image { backface-visibility: hidden; transition: 0.3s ease-in-out; }

.option-animate-image-grayscale .sb-animate-image:hover { filter: grayscale(100%); }

.option-animate-image-grayscale-reverse .sb-animate-image { backface-visibility: hidden; transition: 0.3s ease-in-out; filter: grayscale(100%); }

.option-animate-image-grayscale-reverse .sb-animate-image:hover { filter: grayscale(0); }

