.option-align-center .sb-blog-view__title, .option-align-center .sb-blog-view__date, .option-align-center .sb-blog-view__author { text-align: center; }

.option-align-right .sb-blog-view__title, .option-align-right .sb-blog-view__date, .option-align-right .sb-blog-view__author { text-align: right; }

.option-date-hide-time .sb-blog-view__date-at, .option-date-hide-time .sb-blog-view__date-time { display: none; }
